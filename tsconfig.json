{
  "compilerOptions": {
    "outDir": "dist",
    "lib": ["DOM", "ES2023"],
    "jsx": "react-jsx",
    "module": "commonjs",
    "target": "ES2022",
    "skipLibCheck": true,
    "strict": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "noImplicitThis": false
  }
}
